{
    "name": "Bark Self Build Edit",
    "version": "1.0.0",
    "slug": "bark_server_self_build_edit",
    "description": "Bark Self Build Edit",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "url": "https://github.com/adams549659584/bark-server",
    "host_network": false,
    "ports": {
        "8079/tcp": 8079
    },
    "ports_description": {
        "8079/tcp": "Web interface"
    },
    "map": [
        "config:rw",
        "share",
        "ssl"
    ],
    "options": {
        "configFolder": "Bark",
        "ssl": true,
        "useExternalUrl": true,
        "certfile": "fullchain.pem",
        "keyfile": "privkey.pem"
    },
    "schema": {
        "configFolder": "str",
        "ssl": "bool",
        "useExternalUrl": "bool",
        "certfile": "str",
        "keyfile": "str"
    },
    "panel_icon": "mdi:earth",
    "panel_title": "Bark Self Build Edit",
    "hassio_api": true,
    "hassio_role": "manager",
    "webui": "http://[HOST]:[PORT:8079]",
    "homeassistant_api": false,
    "ingress": true,
    "auth_api": false,
    "docker_api": false,
    "host_dbus": false,
    "apparmor": true,
    "ingress_port": 8079,
    "ingress_entry": ""
}
